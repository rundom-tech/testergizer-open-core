{
  "suiteName": "SauceDemo E2E Suite",
  "tests": [
    {
      "name": "SauceDemo purchase flow",
      "steps": [
        { "action": "open", "value": "/" },
        { "action": "type", "value": "#user-name=standard_user" },
        { "action": "type", "value": "#password=secret_sauce" },
        { "action": "click", "value": "#login-button" },
        { "action": "assertUrl", "value": "/inventory.html" },
        { "action": "assertExists", "value": ".inventory_item" },
        { "action": "click", "value": ".inventory_item button:nth-of-type(1)" },
        { "action": "click", "value": ".inventory_item button:nth-of-type(2)" },
        { "action": "assertExists", "value": ".shopping_cart_badge" },
        { "action": "click", "value": ".shopping_cart_link" },
        { "action": "assertExists", "value": ".cart_item" },
        { "action": "click", "value": "#checkout" },
        { "action": "type", "value": "#first-name=John" },
        { "action": "type", "value": "#last-name=Doe" },
        { "action": "type", "value": "#postal-code=12345" },
        { "action": "click", "value": "#continue" },
        { "action": "click", "value": "#finish" },
        { "action": "assertExists", "value": ".complete-header" }
      ]
    }
  ]
}
